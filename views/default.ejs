<!doctype html5>
<html>
  <head>
    <!-- settings -->
    <meta charset="utf-8">
    <!-- shrink-to-fit: https://forums.developer.apple.com/thread/13510 -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, shrink-to-fit=no">

    <!-- title -->
    <title>
      <%= appName %>
      <% if (clientType !== defaultType) { %>| <%= clientType %><% } %>
    </title>
    <!-- stylesheets -->
    <link rel="stylesheet" href="<%= assetsDomain %>css/main.css">
  </head>

  <body>
    <div id="container" class="container"></div>

    <script>
    window.CONFIG = {};
    window.CONFIG.SOCKET_CONFIG = JSON.parse('<%- socketIO %>');
    window.CONFIG.APP_NAME = '<%- appName %>';
    </script>

    <script src="<%= assetsDomain %>js/<%= clientType %>.js"></script>
  </body>
</html>
